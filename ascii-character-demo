#include <stdio.h>

void print_ascii_table() {
    printf("=== ASCII Character Values ===\n\n");
    
    printf("WHITESPACE:\n");
    printf("' ' (space) = %d\n", ' ');
    
    printf("\nUPPERCASE LETTERS:\n");
    printf("'A' = %d\n", 'A');
    printf("'B' = %d\n", 'B');
    printf("'C' = %d\n", 'C');
    
    printf("\nLOWERCASE LETTERS:\n");
    printf("'a' = %d\n", 'a');
    printf("'b' = %d\n", 'b');
    printf("'c' = %d\n", 'c');
    
    printf("\nDIGITS:\n");
    printf("'0' = %d\n", '0');
    printf("'12' = %d (multi-character constant)\n", '12');
    
    printf("\nSPECIAL SYMBOLS:\n");
    printf("'$' = %d\n", '$');
    printf("'*' = %d\n", '*');
    printf("'+' = %d\n", '+');
    printf("'/' = %d\n", '/');
}

int main() {
    print_ascii_table();
    return 0;
}
